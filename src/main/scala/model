case class Pays private(code: Int,name: String)
	object Pays{
		def build(code: Int, name:String): Pays=
				new Pays(code,name)
}
case class Airport(name: String,pays: String,ville:String,destination:List[String])
  object Airport{
    def build(name: String,pays: String,ville:String,destination:List[String]):Option[Airport]=
      if (pays==test) new Airport(name,pays,ville,destination)
      else 
  }
case class Runway(latitude: double,longitude: double,depart: String,arrivee: String,durre: Int)
  object Runway{
    def build(latitude: double,longitude: double,depart: String,arrivee: String,durre: Int): Option[Runway]=
      if (depart==test) 
          new Runway(latidute,longitude,depart,arrivee,duree)
      else if (arrive==test)
          new Runway(latidute,longitude,depart,arrivee,duree)
    else
  }
